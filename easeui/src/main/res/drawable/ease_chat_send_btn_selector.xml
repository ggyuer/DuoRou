<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#169a13" />

            <corners android:radius="3dp" />


            <stroke android:width="0.5dp" android:color="#0d6900" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#1aad16" />


            <corners android:radius="3dp" />

            <stroke android:width="0.5dp" android:color="#2a9022" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#409240" />

            <corners android:radius="3dp" />
            <stroke android:width="0.5dp" android:color="#0d6900" />
        </shape>
    </item>
</selector><!--<padding android:bottom="2dp" android:left="5dp" android:right="5dp" android:top="2dp" />-->
